        "*.cs",
        "_Legacy/**/*.*",
        "Cache/**/*.*",
        "Controllers/**/*.*",
        "DependencyInjection/**/*.*",
        "Dictionary/**/*.*",
        "Dynamics/**/*.*",
        "Editors/**/*.*",

        "Install/**/*.cs",

        "Macros/**/*.*",
        "Media/**/*.*",
        "Models/**/*.*",
        "Mvc/**/*.*",
        "PropertyEditors/**/*.*",
        "PublishedCache/**/*.*",
        "Routing/**/*.*",
        "Scheduling/**/*.*",
        "Search/**/*.*",
        "Security/**/*.*",
        "Services/**/*.*",
        "Strategies/**/*.*",
