      "excludeFiles": [
        "Install/Controllers/InstallPackageController.cs",
        "Install/FilePermissionHelper.cs",
        "Install/InstallAuthorizeAttribute.cs"
      ],
      "includeFiles": [
        "UmbracoWebStartup.cs",
        "UmbracoContext.cs",

        "Routing/PublishedContentRequest.cs",
        "Routing/PublishedContentRequestEngine.cs",
        "Routing/RoutingContext.cs",
        "Routing/IUrlProvider.cs",
        "Routing/UrlProviderMode.cs",
        "Routing/UrlProvider.cs",
        "Routing/IContentFinder.cs",
        "Routing/ILastChanceContentFinder.cs",

        "PublishedCache/IPublishedCache.cs",
        "PublishedCache/IPublishedContentCache.cs",

        "Models/RouteDefinition.cs",

        "WebApi/AngularJsonOnlyConfigurationAttribute.cs",
        "WebApi/AngularJsonMediaTypeFormatter.cs"
      ]
    }
  },

  "dependencies": {
    "LightInject": "4.1.2",
    "Microsoft.Extensions.Configuration.Abstractions": "1.0.0",
    "System.Text.Encodings.Web": "4.0.0",
    "System.Xml.XmlDocument": "4.0.1",
    "System.Xml.XPath": "4.0.1",
    "System.Xml.XPath.XDocument": "4.0.1",
    "System.ComponentModel.Annotations": "4.1.0",
    "Umbraco.Core": {
      "target": "project",
      "version": "1.0.0-*"
    },
    "AutoMapper": "5.1.1",
    "Microsoft.AspNetCore.DataProtection": "1.0.0",
    "Microsoft.AspNetCore.Identity": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.1",
    "Microsoft.Extensions.Caching.Memory": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Microsoft.Extensions.DependencyModel": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
    "Newtonsoft.Json": "9.0.1",
    "NPoco": "3.4.4",
    "System.Data.Common": "4.1.0",
    "System.Runtime.Serialization.Primitives": "4.1.1"    
  },

  "frameworks": {
    "net461": {
      "dependencies": {
        "MySql.Data": "6.9.9",
